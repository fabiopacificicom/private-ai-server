          "event": [
            {
              "id": "1b3f311d-31c9-4a21-8a53-4cee286f7dd9",
              "listen": "test",
              "script": {
                "type": "text/javascript",
                "exec": [
                  "// 1. Response status code is 200",
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "// 2. Response time is under 3000ms",
                  "pm.test(\"Response time is under 3000ms\", function () {",
                  "    pm.expect(pm.response.responseTime).to.be.below(3000);",
                  "});",
                  "",
                  "// Parse response JSON for subsequent tests",
                  "const jsonData = pm.response.json();",
                  "",
                  "// 3. Response has the correct JSON structure with required fields (object, data)",
                  "pm.test(\"Response has required fields (object, data)\", function () {",
                  "    pm.expect(jsonData).to.have.property(\"object\");",
                  "    pm.expect(jsonData).to.have.property(\"data\");",
                  "});",
                  "",
                  "// 4. The data array exists and has at least one element",
                  "pm.test(\"Data array exists and has at least one element\", function () {",
                  "    pm.expect(jsonData.data).to.be.an(\"array\");",
                  "    pm.expect(jsonData.data.length).to.be.at.least(1);",
                  "});",
                  "",
                  "// 5. Each model object contains required fields (id, object, created, owned_by)",
                  "pm.test(\"Each model object contains required fields (id, object, created, owned_by)\", function () {",
                  "    jsonData.data.forEach(function (model, index) {",
                  "        pm.expect(model).to.have.property(\"id\", \"Model at index \" + index + \" should have id\");",
                  "        pm.expect(model).to.have.property(\"object\", \"Model at index \" + index + \" should have object\");",
                  "        pm.expect(model).to.have.property(\"created\", \"Model at index \" + index + \" should have created\");",
                  "        pm.expect(model).to.have.property(\"owned_by\", \"Model at index \" + index + \" should have owned_by\");",
                  "    });",
                  "});",
                  "",
                  "// 6. Validate that model IDs are non-empty strings",
                  "pm.test(\"Model IDs are non-empty strings\", function () {",
                  "    jsonData.data.forEach(function (model, index) {",
                  "        pm.expect(model.id).to.be.a(\"string\", \"Model ID at index \" + index + \" should be a string\");",
                  "        pm.expect(model.id.length).to.be.above(0, \"Model ID at index \" + index + \" should not be empty\");",
                  "    });",
                  "});",
                  "",
                  "// 7. Validate that created timestamps are numbers",
                  "pm.test(\"Created timestamps are numbers\", function () {",
                  "    jsonData.data.forEach(function (model, index) {",
                  "        pm.expect(model.created).to.be.a(\"number\", \"Created timestamp at index \" + index + \" should be a number\");",
                  "    });",
                  "});",
                  "",
                  "// 8. Check that the object field equals \"list\"",
                  "pm.test(\"Object field equals 'list'\", function () {",
                  "    pm.expect(jsonData.object).to.eql(\"list\");",
                  "});"
                ]
              }
            }
          ],
