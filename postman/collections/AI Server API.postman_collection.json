          "name": "Chat",
          "event": [
            {
              "listen": "test",
              "script": {
                "id": "abe23d88-25b5-4155-917d-f6ba6d8d0a7f",
                "exec": [
                  "// 1. Response status code is 200",
                  "pm.test(\"Status code is 200\", function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "",
                  "// 2. Response time is under 5000ms",
                  "pm.test(\"Response time is under 5000ms\", function () {",
                  "    pm.expect(pm.response.responseTime).to.be.below(5000);",
                  "});",
                  "",
                  "// Parse response JSON for subsequent tests",
                  "const jsonData = pm.response.json();",
                  "",
                  "// 3. Response has the correct JSON structure with required fields",
                  "pm.test(\"Response has required fields (id, object, created, model, choices, usage)\", function () {",
                  "    pm.expect(jsonData).to.have.property(\"id\");",
                  "    pm.expect(jsonData).to.have.property(\"object\");",
                  "    pm.expect(jsonData).to.have.property(\"created\");",
                  "    pm.expect(jsonData).to.have.property(\"model\");",
                  "    pm.expect(jsonData).to.have.property(\"choices\");",
                  "    pm.expect(jsonData).to.have.property(\"usage\");",
                  "});",
                  "",
                  "// 4. The choices array exists and has at least one element",
                  "pm.test(\"Choices array exists and has at least one element\", function () {",
                  "    pm.expect(jsonData.choices).to.be.an(\"array\");",
                  "    pm.expect(jsonData.choices.length).to.be.at.least(1);",
                  "});",
                  "",
                  "// 5. The message object contains role and content fields",
                  "pm.test(\"Message object contains role and content fields\", function () {",
                  "    jsonData.choices.forEach(function (choice, index) {",
                  "        pm.expect(choice).to.have.property(\"message\", \"Choice at index \" + index + \" should have message\");",
                  "        pm.expect(choice.message).to.have.property(\"role\", \"Message at index \" + index + \" should have role\");",
                  "        pm.expect(choice.message).to.have.property(\"content\", \"Message at index \" + index + \" should have content\");",
                  "    });",
                  "});",
                  "",
                  "// 6. The usage object contains prompt_tokens, completion_tokens, and total_tokens",
                  "pm.test(\"Usage object contains prompt_tokens, completion_tokens, and total_tokens\", function () {",
                  "    pm.expect(jsonData.usage).to.be.an(\"object\");",
                  "    pm.expect(jsonData.usage).to.have.property(\"prompt_tokens\");",
                  "    pm.expect(jsonData.usage).to.have.property(\"completion_tokens\");",
                  "    pm.expect(jsonData.usage).to.have.property(\"total_tokens\");",
                  "    pm.expect(jsonData.usage.prompt_tokens).to.be.a(\"number\");",
                  "    pm.expect(jsonData.usage.completion_tokens).to.be.a(\"number\");",
                  "    pm.expect(jsonData.usage.total_tokens).to.be.a(\"number\");",
                  "});",
                  "",
                  "// 7. Validate that the model in response matches the requested model",
                  "pm.test(\"Response model matches requested model\", function () {",
                  "    const requestBody = JSON.parse(pm.request.body.raw);",
                  "    const requestedModel = requestBody.model;",
                  "    pm.expect(jsonData.model).to.include(requestedModel);",
                  "});",
                  "",
                  "// 8. Check that the response content is not empty",
                  "pm.test(\"Response content is not empty\", function () {",
                  "    jsonData.choices.forEach(function (choice, index) {",
                  "        pm.expect(choice.message.content).to.be.a(\"string\");",
                  "        pm.expect(choice.message.content.trim().length).to.be.above(0, \"Content at index \" + index + \" should not be empty\");",
                  "    });",
                  "});"
                ],
                "type": "text/javascript",
                "packages": {},
                "requests": {}
              }
            }
          ],
